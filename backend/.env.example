# Server
PORT=3001

# Database (default: SQLite)
# For SQLite, only DB_STORAGE is needed
DB_DIALECT=sqlite
DB_STORAGE=./database.sqlite

# For PostgreSQL/MySQL, uncomment and fill these:
# DB_DIALECT=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=bitespeed
# DB_USER=your_user
# DB_PASSWORD=your_password
